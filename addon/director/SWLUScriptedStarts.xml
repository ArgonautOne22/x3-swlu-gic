<?xml version="1.0" encoding="ISO-8859-1" ?>
<?xml-stylesheet href="director.xsl" type="text/xsl" ?>
<director name="test" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="director.xsd">
  <documentation>
    <author name="Purls" alias="Purls"/>
    <content reference="Starts SWLU scripted code" name="constants" description="Starts SWLU Scripted starts"/>
    <version number="0.1" date="June 28th 2022" status="development"/>
  </documentation>
  <cues>
	<cue name="Rebel_CellLeader">
      <condition>
        <check_all>
          <check_age min="1s"/>
          <check_value value="{player.gamestart}" exact="135" comment="135 from gamestarts.xml"/>
        </check_all>
      </condition>
      <action>
        <do_all>
          <run_script script="SWLU.InitGamestart.Rebel_Cell" />          
        </do_all>
      </action>
    </cue>
